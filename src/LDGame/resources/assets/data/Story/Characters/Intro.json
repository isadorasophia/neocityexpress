            "Id": 1,
            "Requirements": {
              "$type": "System.Collections.Immutable.ImmutableArray`1[[Murder.Core.Dialogs.CriterionNode, Murder]], System.Collections.Immutable",
              "$values": []
            },
            "Lines": {
              "$type": "System.Collections.Immutable.ImmutableArray`1[[Murder.Core.Dialogs.Line, Murder]], System.Collections.Immutable",
              "$values": [
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "Intro1",
                  "Text": "Hey, y'see that lady? That's my grandma, she founded Neo City Express, a courier service running mail and packages all over Neo City."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "Intro1",
                  "Text": "Within a few years, Neo City Express had become the most trusted delivery service in town. If a package absolutely had to be delivered safely and on time, there was nobody else you could trust."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "3f53f270-850d-4c56-9d91-cad58e7e7c41",
                  "Portrait": "Smirk",
                  "Text": "...but recently Gram-Gram passed away, and Neo City Express suffered. Now the family business has been passed down to me, Daggers McBlack."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "3f53f270-850d-4c56-9d91-cad58e7e7c41",
                  "Portrait": "Smirk",
                  "Text": "Of course, there are some who say I'll never live up to what Gram-Gram achieved. But I'm not intimidated! After all, I'm Daggers McBlack, the most attractive and irrationally confident delivery driver this side of West Scumwood!"
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "3f53f270-850d-4c56-9d91-cad58e7e7c41",
                  "Portrait": "Smirk",
                  "Text": "That's why I've made the completely arbitrary commitment of making Neo City Express the number one delivery service in Neo City within 6 days!"
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "3f53f270-850d-4c56-9d91-cad58e7e7c41",
                  "Portrait": "Smirk",
                  "Text": "Don't worry Gram-Gram, I'll carry the family business on my shoulders!"
                }
              ]
            },
            "IsChoice": false
          }
        ]
      },
