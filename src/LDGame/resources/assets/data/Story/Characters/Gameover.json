            "Id": 1,
            "Requirements": {
              "$type": "System.Collections.Immutable.ImmutableArray`1[[Murder.Core.Dialogs.CriterionNode, Murder]], System.Collections.Immutable",
              "$values": []
            },
            "Lines": {
              "$type": "System.Collections.Immutable.ImmutableArray`1[[Murder.Core.Dialogs.Line, Murder]], System.Collections.Immutable",
              "$values": [
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "HospitalPortrait",
                  "Text": "The explosion landed Daggers in the hospital, where he had to spend three months in recovery."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "HospitalPortrait",
                  "Text": "During this time, his sister came to visit, and spent hours at a time by his side."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "HospitalPortrait",
                  "Text": "The two of them talked endlessly, and grew close in ways they'd never been before."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "HospitalPortrait",
                  "Text": "Daggers was finally able to open up to her about his substance abuse and his thrill-seeking addiction to danger."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "HospitalPortrait",
                  "Text": "He had never been so open or vulnerable before."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "HospitalPortrait",
                  "Text": "When he got out of the hospital, he turned his life around. He committed himself to being a safer, more responsible driver, and never got behind the wheel unless he was completely in control."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "HospitalPortrait",
                  "Text": "Neo City Express never again became the number one delivery service in Neo City, but Daggers didn't mind. After all, he had his family, and the security of knowing that he was finally taking care of himself."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "HospitalPortrait",
                  "Text": "He lived a long and happy life."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "HospitalPortrait",
                  "Text": "Congratulations! You got the GOOD ENDING!"
                }
              ]
            },
            "IsChoice": false
          }
        ]
      },
