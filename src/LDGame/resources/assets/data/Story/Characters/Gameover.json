            "Id": 1,
            "Requirements": {
              "$type": "System.Collections.Immutable.ImmutableArray`1[[Murder.Core.Dialogs.CriterionNode, Murder]], System.Collections.Immutable",
              "$values": []
            },
            "Lines": {
              "$type": "System.Collections.Immutable.ImmutableArray`1[[Murder.Core.Dialogs.Line, Murder]], System.Collections.Immutable",
              "$values": [
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "Handshake",
                  "Text": "The mayor was so impressed with Daggers' delivery skills that he awarded him the city's highest honor: a warm hug."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "Handshake",
                  "Text": "He also gave Neo City Express the exclusive contract to deliver all packages for the government, until the literal end of time."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "Handshake",
                  "Text": "Daggers became a hero, and he spent the rest of his life making fast and reliable deliveries for clients all over Neo City."
                },
                {
                  "$type": "Murder.Core.Dialogs.Line, Murder",
                  "Speaker": "1a9ec707-943d-43e4-b9fc-55e6dfe7ba4b",
                  "Portrait": "Handshake",
                  "Text": "And yet...."
                }
              ]
            },
            "IsChoice": false
          }
        ]
      },
